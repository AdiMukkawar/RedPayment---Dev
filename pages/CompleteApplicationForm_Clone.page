                    var key = event.keyCode || event.charCode;                      
                    if(valueArray != null && valueArray != ""){                         
                        if(valueArray.length == 2){
                            if( key != 8 && key != 46 ){
                                valueArray = valueArray.substring(0,2)+'/';                                 
                            }
                        }else if(valueArray.length == 5){
                            if( key != 8 && key != 46 ){
                                valueArray = valueArray.substring(0,5)+'/';
                            }
                        }else if(valueArray.length >= 6){
                            if( key != 8 && key != 46 ){
                                valueArray = valueArray.substring(0,10);
                            }
                        }
                    }
                    inputVal.value = valueArray;
                }
                
